{
  "singleQuote": true,
  "trailingComma": "none",
  "jsxBracketSameLine": false,
  "bracketSpacing": true,
  "printWidth": 100,
  "semi": false,
  "arrowParens": "avoid",
  "importOrder": ["^~/(.*)$", "^[./]"],
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderSeparation": true
}
